<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.pims.model.CommunityMaster" table="EGEIS_COMMUNITY_MSTR">
		
		<id
							name="id"
							column="COMMUNITY_ID"
							type="int">
							<generator class="sequence">
								<param name="sequence">EGPIMS_COMMUNITY_SEQ</param>
							</generator>
				</id>
		
		<property
			name="name"
			column="COMMUNITY_NAME"
			
		/>
		<property
							name="fromDate"
							column="START_DATE"
							
				/>
				<property
							name="toDate"
							column="END_DATE"
							
		/>
	</class>
</hibernate-mapping>