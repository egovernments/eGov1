<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.pims.model.StatusMaster" table="EGEIS_LEAVE_STATUS">
		
		<id
							name="id"
							column="ID"
							type="int">
							<generator class="sequence">
								<param name="sequence">EGPIMS_RELIGION_SEQ</param>
							</generator>
		</id>
		<property
			name="name"
			column="STATUS"
			
		/>
		
	</class>
</hibernate-mapping>