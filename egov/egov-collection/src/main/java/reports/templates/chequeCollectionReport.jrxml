	</parameter>
	<queryString>
		<![CDATA[SELECT CH.ID AS COLLECTIONHEADER_ID,
	CH.RECEIPTNUMBER AS RECEIPT_NUMBER,
	CH.CONSUMERCODE,
     	ED.DEPT_NAME,
	ES.SERVICENAME,
	ECPD.PAYEENAME,
	IH.INSTRUMENTAMOUNT,
	IH.INSTRUMENTNUMBER,
	IH.INSTRUMENTDATE,
	BK.NAME AS INSTRUMENTBANK,
	EGCL_CHALLANHEADER."CHALLANNUMBER" AS CHALLAN_NUMBER,
	IH.BANKBRANCHNAME AS INSTRUMENTBRANCH , 
	DECODE(CJV.TOBANKACCOUNTID,NULL,' ',(SELECT B1.NAME FROM BANK B1,BANKBRANCH BB1,BANKACCOUNT BA1 WHERE B1.ID=BB1.BANKID AND BB1.ID=BA1.BRANCHID AND BA1.ID=CJV.TOBANKACCOUNTID)) AS REMITBANK,
	DECODE(CJV.TOBANKACCOUNTID,NULL,'',(SELECT BB2.BRANCHNAME FROM BANKBRANCH BB2,BANKACCOUNT BA2 WHERE
BB2.ID=BA2.BRANCHID AND BA2.ID=CJV.TOBANKACCOUNTID)) AS REMITBRANCH , DECODE(CJV.TOBANKACCOUNTID,NULL,'',(SELECT BA3.ACCOUNTNUMBER FROM BANKACCOUNT BA3 WHERE
BA3.ID=CJV.TOBANKACCOUNTID)) AS REMITACCOUNT
FROM 
	EGCL_COLLECTIONHEADER CH left join EGCL_CHALLANHEADER on EGCL_CHALLANHEADER.RECEIPTID = CH.ID,
	EGCL_COLLECTIONPAYEEDETAILS ECPD,
	EGCL_COLLECTIONMIS CM LEFT JOIN EG_BOUNDARY EB on CM.ID_BOUNDARY=EB.ID_BNDRY,
	
	EG_DEPARTMENT ED,EG_SERVICEDETAILS ES,
	EGCL_COLLECTIONINSTRUMENT CI,
	EGF_INSTRUMENTHEADER IH left join CONTRAJOURNALVOUCHER CJV on CJV.INSTRUMENTHEADERID=IH.ID,
        BANK BK, EGW_STATUS ST
WHERE 
	ED.ID_DEPT=CM.ID_DEPARTMENT AND CM.ID_COLLECTIONHEADER=CH.ID
	AND CH.ID_SERVICE=ES.ID AND CI.COLLECTIONHEADERID=CH.ID
	AND IH.ID=CI.INSTRUMENTMASTERID AND IH.BANKID=BK.ID
	AND IH.INSTRUMENTTYPE IN (SELECT ID FROM EGF_INSTRUMENTTYPE WHERE TYPE IN ('cheque','dd'))
        AND IH.ID_STATUS = ST.ID
        AND ST.DESCRIPTION != 'Cancelled'
	AND ECPD.ID=CH.ID_COLLECTIONPAYEEDETAILS
	AND CH.CREATED_DATE >= $P{EGOV_FROM_DATE}
	AND CH.CREATED_DATE < $P{EGOV_TO_DATE_PLUS_ONE}
	$P!{Q_COUNTER_ID}
	$P!{Q_COUNTER_OPERATOR_ID}
	$P!{Q_BOUNDARY_ID}
	$P!{Q_RECEIPT_IDS}
	ORDER BY RECEIPT_NUMBER]]>
	</queryString>
