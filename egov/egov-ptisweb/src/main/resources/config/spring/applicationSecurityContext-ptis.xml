				<security:filter-chain pattern="/citizen/search/searchproperty-demolition.action" filters="none" />
				<security:filter-chain pattern="/citizen/property/demolition/**" filters="none" />
				<security:filter-chain pattern="/citizen/demolition/ack/**" filters="none" />
				<security:filter-chain pattern="/citizen/search/searchproperty-taxexemption.action" filters="none" />
				<security:filter-chain pattern="/citizen/exemption/form/**" filters="none" />
				<security:filter-chain pattern="/citizen/exemption/printAck/**" filters="none" />
