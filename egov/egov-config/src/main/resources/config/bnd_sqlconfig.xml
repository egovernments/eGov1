<?xml version="1.0" encoding="iso-8859-1" ?>

<!-- Configuration file for Egov Applications -->

<properties>  
   
     <sql>   
       
       
       	<bnd-state> 
       		<type>hql</type> 
       		<query>from org.egov.mdm.masters.administration.State state order by state.name</query>
        </bnd-state> 
       
        <bnd-district> 
       		<type>hql</type> 
